{
    "name": "Skip",
    "version": "1.0.0",
    "description": "skip ads",
    "manifest_version": 3,
    "author": "Rahul Kumar Mohanta",
    "action":{
        "default_popup": "index.html",
        "default_title": "Skip"
    },
    "permissions": ["storage", "declarativeContent", "activeTab", "scripting"],
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["script.js"]
        }
      ]
}